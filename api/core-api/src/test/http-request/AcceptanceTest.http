### 1-1. ROLE_USER 회원 로그인
< {%
    client.global.set("loginId", "user001")
    client.global.set("password", "1234")

    request.variables.set("loginId", client.global.get("loginId"))
    request.variables.set("password", client.global.get("password"))
%}
POST {{host}}/api/v1/auth/signin
Content-Type: application/json

{
  "loginId": "{{loginId}}",
  "password": "{{password}}"
}

> {%
    client.test("1-1. ROLE_USER 회원 로그인 - 200", function () {
        client.assert(response.status === 200, response.status);
    });

    client.global.set("userRoleToken", response.body["accessToken"])
%}


### 1-2. ROLE=USER 권한이 필요한 api 호출
# @no-cookie-jar
GET {{host}}/api/v1/test/auth/user
Authorization: Bearer {{userRoleToken}}

> {%
    client.test("1-2. ROLE=USER 권한이 필요한 api 호출 - 200", function () {
        client.assert(response.status === 200, response.status + ", userRoleToken=" + client.global.get("userRoleToken"));
    });
%}


### 1-3. ROLE=banker 권한이 필요한 api 호출
GET {{host}}/api/v1/test/auth/banker
Authorization: Bearer {{userRoleToken}}

> {%
    client.test("1-3. ROLE=banker 권한이 필요한 api 호출 - 403", function () {
        client.assert(response.status === 403);
    });
%}


